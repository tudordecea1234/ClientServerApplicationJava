<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.CharityCase"
           table="CharityCases">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property
                name="caseName"
                column="caseName"/>
        <property
                name="totalAmount"
                column="totalAmount"/>
    </class>
</hibernate-mapping>